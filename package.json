{
  "name": "oniyi-utils",
  "version": "1.1.1",
  "description": "Collection of utils for the `oniyi` ecosystem",
  "license": "MIT",
  "private": false,
  "author": "Benjamin Kroeger <benjamin.kroeger@gmail.com> (http://benjamin-kroeger.de)",
  "main": "lib/index.js",
  "keywords": [
    "oniyi",
    "utils",
    "tools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benkroeger/oniyi-utils.git"
  },
  "scripts": {
    "coverage": "nyc npm test && nyc report --reporter=html",
    "format": "prettier-eslint --write \"lib/**/*.js\" \"test/**/*.js\"",
    "prelint": "npm run format",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "ava --verbose",
    "test:watch": "npm test -- --watch"
  },
  "engines": {
    "node": ">=v6.6.0"
  },
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "ava": "^0.23.0",
    "eslint": "^4.10.0",
    "eslint-config-oniyi": "^5.0.2",
    "eslint-plugin-ava": "^4.2.2",
    "nyc": "^11.3.0",
    "prettier-eslint-cli": "^4.4.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "lodash.union": "^4.6.0"
  },
  "nyc": {
    "exclude": [
      "node_modules/**",
      "test/**"
    ]
  },
  "bugs": {
    "url": "https://github.com/benkroeger/oniyi-utils/issues"
  },
  "homepage": "https://github.com/benkroeger/oniyi-utils#readme",
  "directories": {
    "test": "test"
  }
}
