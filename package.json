{
  "name": "oniyi-utils",
  "version": "1.0.0",
  "description": "Collection of utils for the `oniyi` ecosystem",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Benjamin Kroeger",
    "email": "benjamin.kroeger@gmail.com",
    "url": "http://benjamin-kroeger.de"
  },
  "main": "lib/index.js",
  "keywords": [
    "oniyi",
    "utils",
    "tools"
  ],
